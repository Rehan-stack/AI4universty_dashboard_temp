{
  "version": 2,
  "builds": [
    {
      "src": "writoai/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "projectname/wsgi.py"
    },
    
  ]
}
{
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
